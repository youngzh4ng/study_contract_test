{
  "consumer": {
    "name": "client"
  },
  "provider": {
    "name": "ProductService"
  },
  "interactions": [
    {
      "description": "a request to retrieve product list",
      "providerState": "it has one product",
      "request": {
        "method": "GET",
        "path": "/products"
      },
      "response": {
        "status": 200,
        "headers": {
        },
        "body": [
          {
            "name": "Foo",
            "img": "https://webshop.com/img/foo.png",
            "price": 1,
            "stock": 1
          }
        ]
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}